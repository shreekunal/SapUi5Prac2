<core:FragmentDefinition xmlns:core="sap.ui.core" xmlns:u="sap.ui.unified" xmlns="sap.m">
    <Dialog id="_IDGenDialog1" title="Upload" class="sapUiResponsiveContentPadding"
        beforeOpen=".onBeforeOpen" afterClose=".onAfterClose">
        <content>
            <u:FileUploader
                id="uploader"
                fileType="xlsx"
                multiple="false"
                uploadUrl="/odata/v4/processor/Upload/excel"
                fileAllowed=".onFileAllowed"
                fileEmpty=".onFileEmpty"
                uploadComplete=".onUploadComplete"
                typeMissmatch=".onTypeMismatch"
                sendXHR="true"
                useMultipart="false"
                placeholder="Choose a xlsx file..."
                httpRequestMethod="Put"
                name="Incidents"
            />
        </content>
        <beginButton>
            <Button id="ok" text="OK" press=".onOk" type="Emphasized" enabled="false" />
        </beginButton>
        <endButton>
            <Button id="cancel" text="Cancel" press=".onCancel" />
        </endButton>
    </Dialog>
</core:FragmentDefinition>